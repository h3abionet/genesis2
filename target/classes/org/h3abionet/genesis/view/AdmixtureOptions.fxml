<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="210.0" prefWidth="350.0" stylesheets="@../css/AdmixtureOptions.css" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.h3abionet.genesis.controller.AdmixtureOptionsController">
   <children>
      <VBox alignment="CENTER" prefHeight="203.0" prefWidth="299.0" spacing="5.0" style="-fx-border-color: #e0e0e0; -fx-border-radius: 5;" AnchorPane.bottomAnchor="10.0" AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="10.0" AnchorPane.topAnchor="10.0">
         <children>
            <Button fx:id="populationGroupOptionsBtn" layoutX="10.0" layoutY="10.0" mnemonicParsing="false" onAction="#loadPopulationGroupOptions" prefHeight="27.0" prefWidth="420.0" text="Population Group Options (Colours)">
               <font>
                  <Font name="Helvetica" size="14.0" />
               </font></Button>
            <Button fx:id="previousGraphColourBtn" layoutX="10.0" layoutY="37.0" mnemonicParsing="false" onAction="#colourLikePrevious" prefHeight="27.0" prefWidth="385.0" text="Colour this graph similar to previous ">
               <font>
                  <Font name="Helvetica" size="14.0" />
               </font></Button>
            <Button fx:id="nextGraphColourBtn" layoutX="10.0" layoutY="64.0" mnemonicParsing="false" onAction="#colourLikeNext" prefHeight="27.0" prefWidth="398.0" text="Colour this graph similar to next">
               <font>
                  <Font name="Helvetica" size="14.0" />
               </font></Button>
            <HBox prefHeight="81.0" prefWidth="318.0" spacing="10.0" style="-fx-border-color: #e0e0e0; -fx-border-radius: 5;">
               <children>
                  <VBox prefHeight="200.0" prefWidth="144.0" spacing="2.0">
                     <children>
                        <Button fx:id="shiftUpBtn" mnemonicParsing="false" onAction="#shiftGraphUp" style="-fx-border-color: #e0e0e0; -fx-border-radius: 5;" text="Shift Graph Up">
                           <font>
                              <Font name="Helvetica" size="14.0" />
                           </font></Button>
                        <Button fx:id="shiftDownBtn" mnemonicParsing="false" onAction="#shiftGraphDown" prefHeight="27.0" prefWidth="141.0" style="-fx-border-color: #e0e0e0; -fx-border-radius: 5;" text="Shift Graph Down">
                           <font>
                              <Font name="Helvetica" size="14.0" />
                           </font></Button>
                     </children>
                  </VBox>
                  <VBox prefHeight="54.0" prefWidth="175.0" spacing="2.0">
                     <children>
                        <Button fx:id="shiftTopBtn" mnemonicParsing="false" onAction="#shiftGraphTop" prefHeight="28.0" prefWidth="159.0" style="-fx-border-color: #e0e0e0; -fx-border-radius: 5;" text="Shift Graph to Top">
                           <font>
                              <Font name="Helvetica" size="14.0" />
                           </font></Button>
                        <Button fx:id="shiftBottomBtn" mnemonicParsing="false" onAction="#shiftGraphBottom" prefHeight="27.0" prefWidth="161.0" style="-fx-border-color: #e0e0e0; -fx-border-radius: 5;" text="Shift Graph to Bottom">
                           <font>
                              <Font name="Helvetica" size="14.0" />
                           </font></Button>
                     </children>
                     <padding>
                        <Insets bottom="2.0" left="2.0" right="2.0" top="2.0" />
                     </padding>
                  </VBox>
               </children>
               <VBox.margin>
                  <Insets />
               </VBox.margin>
               <padding>
                  <Insets bottom="2.0" left="2.0" right="2.0" top="2.0" />
               </padding>
            </HBox>
            <Button fx:id="deleteGraphBtn" layoutX="10.0" layoutY="91.0" mnemonicParsing="false" onAction="#deleteGraph" prefHeight="27.0" prefWidth="426.0" text="Delete Graph">
               <font>
                  <Font name="Helvetica" size="14.0" />
               </font></Button>
            <Button fx:id="cancelBtn" mnemonicParsing="false" onAction="#cancelHandler" prefHeight="29.0" prefWidth="107.0" style="-fx-border-color: #e0e0e0; -fx-border-radius: 5;" text="Cancel">
               <VBox.margin>
                  <Insets />
               </VBox.margin>
               <font>
                  <Font name="Helvetica" size="14.0" />
               </font>
            </Button>
         </children>
         <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </padding>
      </VBox>
   </children>
</AnchorPane>
