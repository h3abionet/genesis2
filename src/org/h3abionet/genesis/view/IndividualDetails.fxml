                              <Button fx:id="btnCancel" mnemonicParsing="false" onAction="#entryCancelButton" prefHeight="25.0" prefWidth="81.0" text="Cancel" />
                           </children>
                        </HBox>
                     </children>
                     <VBox.margin>
                        <Insets />
                     </VBox.margin>
                     <effect>
                        <DropShadow height="5.0" radius="6.0" />
