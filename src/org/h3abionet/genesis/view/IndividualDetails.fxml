                     </effect>
                  </AnchorPane>
                  <Label prefHeight="17.0" prefWidth="123.0" text="Phenotype Data:">
                     <font>
                        <Font size="13.0" />
                     </font>
                     <VBox.margin>
                        <Insets left="90.0" right="90.0" />
                     </VBox.margin>
                  </Label>
                  <AnchorPane prefHeight="200.0" prefWidth="200.0" style="-fx-background-color: #ebedf0;">
                     <children>
                        <Label fx:id="phenoLabel" prefHeight="121.0" prefWidth="220.0" AnchorPane.leftAnchor="0.0">
                           <effect>
                              <DropShadow />
                           </effect>
                        </Label>
                     </children>
                     <VBox.margin>
                        <Insets left="30.0" right="50.0" />
                     </VBox.margin>
                     <effect>
                        <DropShadow height="5.0" radius="2.0" width="5.0" />
                     </effect>
                  </AnchorPane>
                  <AnchorPane prefHeight="83.0" prefWidth="300.0">
