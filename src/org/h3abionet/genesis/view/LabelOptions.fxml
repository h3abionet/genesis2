<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.ColorPicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.effect.DropShadow?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane id="AnchorPane" prefHeight="222.0" prefWidth="250.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.h3abionet.genesis.controller.LabelOptionsController">
   <children>
      <VBox layoutX="64.0" layoutY="50.0" prefHeight="212.0" prefWidth="250.0" spacing="10.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <AnchorPane style="-fx-background-color: #ebeced;">
               <VBox.margin>
                  <Insets />
               </VBox.margin>
               <children>
                  <Label text="Enter new label text" AnchorPane.leftAnchor="60.0" AnchorPane.rightAnchor="68.0" AnchorPane.topAnchor="5.0" />
               </children>
               <effect>
                  <DropShadow height="5.0" radius="6.0" />
               </effect>
            </AnchorPane>
            <Label fx:id="labelHandler" prefHeight="40.0" prefWidth="190.0" style="-fx-border-color: #8e9194;" textAlignment="CENTER">
               <VBox.margin>
                  <Insets bottom="5.0" left="30.0" right="30.0" top="5.0" />
               </VBox.margin>
            </Label>
            <Button fx:id="fontBtn" mnemonicParsing="false" onAction="#setLabelFont" prefHeight="32.0" prefWidth="150.0" text="Change Label Font">
               <VBox.margin>
                  <Insets bottom="5.0" left="50.0" right="50.0" top="5.0" />
               </VBox.margin>
            </Button>
            <HBox prefHeight="32.0" prefWidth="190.0">
               <VBox.margin>
                  <Insets bottom="5.0" left="30.0" right="30.0" top="5.0" />
               </VBox.margin>
               <children>
                  <CheckBox fx:id="deleteBox" mnemonicParsing="false" prefHeight="18.0" prefWidth="117.0" text="Delete" />
                  <ColorPicker fx:id="labelColorPicker" onAction="#setLabelColor" />
               </children>
            </HBox>
            <AnchorPane style="-fx-background-color: #ebeced;">
               <VBox.margin>
                  <Insets />
               </VBox.margin>
               <children>
                  <HBox layoutY="19.0" prefHeight="37.0" prefWidth="190.0" spacing="60.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="30.0" AnchorPane.rightAnchor="30.0" AnchorPane.topAnchor="0.0">
                     <children>
                        <Button fx:id="doneBtn" mnemonicParsing="false" onAction="#entryDoneBtn" prefHeight="27.0" prefWidth="62.0" text="Done">
                           <HBox.margin>
                              <Insets bottom="5.0" top="5.0" />
                           </HBox.margin>
                        </Button>
                        <Button fx:id="cancelBtn" layoutX="10.0" layoutY="10.0" mnemonicParsing="false" onAction="#entryCancelBtn" prefHeight="27.0" prefWidth="70.0" text="Cancel">
                           <HBox.margin>
                              <Insets bottom="5.0" top="5.0" />
                           </HBox.margin>
                        </Button>
                     </children>
                  </HBox>
               </children>
               <effect>
                  <DropShadow height="5.0" radius="6.0" />
               </effect>
            </AnchorPane>
         </children>
      </VBox>
   </children>
</AnchorPane>
